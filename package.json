{
  "name": "fetch-unfiller",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    },
    "./node": {
      "require": "./lib/node.cjs",
      "import": "./lib/node.mjs"
    }
  },
  "packageManager": "pnpm@8.2.0",
  "engines": {
    "node": ">=16"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,json5,yaml,yml,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint lib",
    "test": "vitest",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@types/node-fetch": "^2 || ^3"
  },
  "peerDependenciesMeta": {
    "@types/node-fetch": {
      "optional": true
    }
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "2.1.0",
    "@tsconfig/node16": "1.0.3",
    "@tsconfig/strictest": "2.0.0",
    "@types/node-fetch": "2.6.3",
    "@vitest/coverage-c8": "0.30.0",
    "dotenv": "16.0.3",
    "eslint": "8.38.0",
    "eslint-define-config": "1.17.0",
    "lint-staged": "13.2.1",
    "prettier": "2.8.7",
    "simple-git-hooks": "2.8.1",
    "tsup": "6.7.0",
    "typescript": "5.0.4",
    "vite": "4.2.1",
    "vitest": "0.30.0"
  }
}
